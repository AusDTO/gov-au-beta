<%- if field.data.cms_type == 'GovCMS' %>
    <%= form_tag import_admin_section_path(id: field.data),
        method: "post",
        data: {
            confirm: t("administrate.actions.confirm"),
            disable_with: "Importing, please wait..."
        } do %>
        <%= submit_tag("Import from CMS") %>
    <% end %>
<% end %>
